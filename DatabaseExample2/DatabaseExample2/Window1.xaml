<Window x:Class="DatabaseExample2.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DatabaseExample2"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="lstNames" HorizontalAlignment="Left" Height="115" Margin="142,99,0,0" VerticalAlignment="Top" Width="344"/>
        <Button Content="Data Reader" HorizontalAlignment="Left" Margin="142,234,0,0" VerticalAlignment="Top" Width="273" Height="36" Click="Button_Click"/>
        <Button Content="MARS" HorizontalAlignment="Left" Margin="442,227,0,0" VerticalAlignment="Top" Width="167" Height="43" Click="Button_Click_1"/>
        <Button Content="Read Data Reader From a function" HorizontalAlignment="Left" Margin="86,298,0,0" VerticalAlignment="Top" Width="291" RenderTransformOrigin="0.5,0.5" Height="39" Click="Button_Click_2">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.353"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>

    </Grid>
